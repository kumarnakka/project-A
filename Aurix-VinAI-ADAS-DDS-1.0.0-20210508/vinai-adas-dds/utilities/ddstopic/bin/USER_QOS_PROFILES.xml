<?xml version="1.0"?>
<dds
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/6.0.0/rti_dds_qos_profiles.xsd"
     version="6.0.0">
	<qos_library name="Autopilot">
		<qos_profile name="CameraImage_Profile" base_name="BuiltinQosLibExp::Generic.StrictReliable.LargeData" is_default_qos="false">
			<participant_qos>
				<participant_name>
					<name>cameraParticipant</name>
					<role_name>cameraParticipantRole</role_name>
				</participant_name>
				<transport_builtin>
					<mask>SHMEM|UDPv4</mask>
				</transport_builtin>
				<discovery>
					<initial_peers>
						<element>shmem://</element>
						<element>127.0.0.1</element>
						<element>192.168.20.45</element>
					</initial_peers>
				</discovery>
				<property>
					<value>
						<element>
							<name>dds.transport.shmem.builtin.received_message_count_max</name>
							<value>100</value>
						</element>
						<element>
							<name>dds.transport.shmem.builtin.receive_buffer_size</name>
							<value>60000000</value>
						</element>
					</value>
				</property>
				<resource_limits>
					<max_destination_ports>32</max_destination_ports>
					<max_receive_ports>32</max_receive_ports>
				</resource_limits>
			</participant_qos>
			<datawriter_qos>
				<resource_limits>
					<max_samples>1</max_samples>
					<max_instances>1</max_instances>
					<max_samples_per_instance>1</max_samples_per_instance>	
				</resource_limits>
				<history>
					<kind>DDS_KEEP_LAST_HISTORY_QOS</kind>
					<depth>1</depth>	
				</history>		
				<reliability>
					<kind>RELIABLE_RELIABILITY_QOS</kind>
				</reliability>	
				<publication_name>
					<name>cameraDriver</name>
				</publication_name>
				<publish_mode>
					<kind>ASYNCHRONOUS_PUBLISH_MODE_QOS</kind>
				</publish_mode>
			</datawriter_qos>
			<datareader_qos>
				<resource_limits>
					<max_samples>1</max_samples>
					<max_instances>1</max_instances>
					<max_samples_per_instance>1</max_samples_per_instance>	
				</resource_limits>
				<reader_resource_limits>
					<max_remote_writers>16</max_remote_writers>
					<max_remote_writers_per_instance>1</max_remote_writers_per_instance>	
				</reader_resource_limits>
				<history>
					<kind>DDS_KEEP_LAST_HISTORY_QOS</kind>
					<depth>1</depth>	
				</history>	
				<subscription_name>
					<name>cameraReader</name>
				</subscription_name>
			</datareader_qos>
		</qos_profile>
	</qos_library>
</dds>
